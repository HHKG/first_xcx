/* 悬浮按钮 */

.holiday-suspend-btn {
  width:125rpx;
  height: 127rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  text-align: center;
  position: fixed;
  bottom: 125rpx;
  right: 30rpx;
  z-index: 11;
  justify-content: center;
}

.animation-hide {
  animation: hideBlock 1s forwards;
}

.animation-show {
  animation: showBlock 1s forwards;
}

@keyframes hideBlock {
  0% {
    right: 4%;
  }

  100% {
    right: -24%;
  }
}

@keyframes showBlock {
  0% {
    right: -24%;
  }

  100% {
    right: 4%;
  }
}
