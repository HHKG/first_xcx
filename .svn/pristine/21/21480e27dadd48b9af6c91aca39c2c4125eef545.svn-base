/* pages/club/index.wxss */

page {
  background-color: #fce8c5;
}
.club-cnt{
padding:0 32rpx;
}
.club-img {
  width: 750rpx;
  height: 520rpx;
}

.club-title {
  position: relative;
  width: 692rpx;
  margin: 70rpx auto 0rpx;
}

.club-title-img {
  width: 692rpx;
  height: 1960rpx;
}

.club-title-cnt {
  position: absolute;
  top: 80rpx;
  left: 32rpx;
}

.club-view {
  display: flex;
  flex-wrap: wrap;
  margin-bottom: 12rpx;
}

.club-view-img {
  margin-left: 12rpx;
  margin-top: 12rpx;
  width: 296rpx;
  height: 230rpx;
  border-radius: 10rpx;
}

.club-view-txt {
  margin: 0rpx 0rpx 12rpx 14rpx;
  width: 600rpx;
  font-size: 28rpx;
  color: #382009;
  line-height: 50rpx;
}

.club-photo {
  width: 618rpx;
  height: 356rpx;
  border-radius: 10rpx;
  margin-left:5rpx;
}
.club-freedom{
  margin-top:50rpx;
  width:688rpx;
  height:607rpx;
}
.club-title2{
  margin-top:66rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.club-title2-img{
  width:665rpx;
  height:40rpx;
  margin-bottom: 40rpx;
}
.club-title2-cnt-img{
  width:692rpx;
  height:200rpx;
}
.club-title3-img{
  width:687rpx;
  height:154rpx;
  margin-bottom: 23rpx;
}
.club-title3-cnt{
  display: flex;
  flex-direction: column;
  align-items: center;
  width:690rpx;
  height:670rpx;
  background-color:#FCE8C5;
  box-shadow: 0rpx 0rpx 30rpx 0rpx rgba(242, 188, 93, 1) 
}
.club-title3-img1{
  margin-top:40rpx;
  width:654rpx;
  height:314rpx;
}
.club-title3-img2{
  margin-top:50rpx;
  width:477rpx;
  height:68rpx;
}
.club-title3-img3{
  margin-top:14rpx;
  width:480rpx;
  height:132rpx;
}
.foreshow-footer{
  display: flex;
  justify-content: space-between;
  padding:10rpx 20rpx 28rpx 20rpx;
}
.foreshow-footer-cnt{
display: flex;
align-items: center;
}
.footer{
 background-color:#fff;
 padding:30rpx 0rpx;
 border-radius: 10rpx;
 margin:30rpx 0rpx;
}
.footer-cnt{
  width:100%;
display: flex;
justify-content: center;
align-items: center;
}
.footer-title{
font-size:32rpx;
font-weight:500;
color:#333;
line-height:45rpx;
margin-left:10rpx;
}
.footer-img{
  width:150rpx;
  height:50rpx;
}
.footer-address{
font-size:24rpx;
font-weight:400;
color:#666;
line-height:45rpx;
text-align: center;
}
.footer-phone{
  color:#E6B854;
}
.foreshow-footer-cnt text{
font-size:26rpx;
font-weight:400;
color:#999;
line-height:37rpx;
margin-left:10rpx;
}