/* pages/form/form.wxss */
@import "/pages/member/index.wxss";
.form-title {
  display: flex;
  align-items: center;
  width: 100%;
  height: 200rpx;
  background: #1b5d59;
  color: #fff;
  font-size: 24rpx;
}

.form-title-layout {
  margin: 0 auto;
  width: 80%;
}

.form-progress {
  border-radius: 100px;
  overflow: hidden;
}

.form-percent {
  color: #f5ce87;
  font-size: 36rpx;
}

.form-progress-num {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 66rpx;
  height: 41rpx;
  background: rgba(27, 78, 81, 1);
  color: #fff;
  font-size: 28rpx;
}

.form-sanjiao {
  width: 0;
  height: 0;
  border-top: 10rpx solid rgba(27, 78, 81, 1);
  border-left: 10rpx solid transparent;
  border-right: 10rpx solid transparent;
}

.form-prompt {
  width: 66rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-left: 63%;
}

.form-btn-img {
  width: 422rpx;
  height: 78rpx;
}

.form-btn {
  margin-top: 80rpx;
  display: flex;
  justify-content: center;
}

.input-name {
  width: 56rpx;
  height: 56rpx;
}

input {
  margin-left: 24rpx;
}

.from-input {
  display: flex;
  align-items: center;
  border-bottom: 1rpx solid #e0e0e0;
  padding: 47rpx 46rpx;
  font-size: 32rpx;
}
.submit-btn {
  margin-top: 324rpx;
  display: flex;
  justify-content: center;
}

.submit-btn-img {
  width: 686rpx;
  height: 114rpx;
}

.mask {
  position: fixed;
  top: 0;
  bottom: 0;
  background: #333;
  width: 100%;
  opacity: 0.5;
}

.form-pop {
  position: fixed;
  top: 285rpx;
  left: 105rpx;
  width: 540rpx;
  height: 580rpx;
  z-index: 1111;
}

.form-pop-msg {
  background: #fff;
  width: 540rpx;
  height: 468rpx;
  border-radius: 10rpx;
  display: flex;
  justify-content: center;
}

.form-pop-img {
  margin-top:84rpx;
  width: 310rpx;
  height: 320rpx;
}

.close-icon {
  margin-top: 42rpx;
  display: flex;
  justify-content: center;
}
.close-icon-img {
  width: 70rpx;
  height: 70rpx;
}
.toast{
  width:50%;
  position: fixed;
  bottom: 30%;
  left:25%;
  background:#000;
  border-radius: 10rpx;
  color:#fff;
  font-size: 30rpx;
  opacity: 0.5;
  text-align: center;
  padding:25rpx 0rpx;
}
