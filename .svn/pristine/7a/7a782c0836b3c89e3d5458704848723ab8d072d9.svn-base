<!--pages/member/cash/authentication/authentication.wxml-->
<view>
  <view class="aut-title">我们将会通过第三方认证您的身份信息</view>
  <view class="aut-h">
    <view class="aut-cnt">
      <text class="aut-cnt-txt">姓名</text>
      <input class="aut-cnt-input" type="text" value="" placeholder='请输入您的姓名'></input>
      <view class="aut-line"></view>
    </view>
    <view class="aut-cnt">
      <text class="aut-cnt-txt">身份证号</text>
      <input class="aut-cnt-input" type="idcard" value="" placeholder='请输入您的身份证号码'></input>
    </view>
  </view>
  <button disabled="{{disabled}}">下一步</button>
</view>