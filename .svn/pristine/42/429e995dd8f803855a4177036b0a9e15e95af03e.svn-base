/* pages/index/index.wxss */
@import "/pages/member/index.wxss";
page {
  background-color: #fce8c5;
  padding-bottom:30rpx;
}

.slide-image {
  width: 750rpx;
  height: 460rpx;
}

.member-say {
  bottom: -20rpx;
}

.foreshow {
  background-color: #fff;
  border-radius: 10rpx;
  overflow: hidden;
  margin-bottom: 30rpx;
}

.foreshow-img {
  width: 690rpx;
  height: 388rpx;
}

.partner {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}

.partner-img {
  margin-bottom: 12rpx;
  width: 222rpx;
  height: 108rpx;
}

.foreshow-position {
  width: 23rpx;
  height: 28rpx;
}

.foreshow-txt {
  margin-left: 20rpx;
  font-size: 32rpx;
  font-weight: 500;
  color: #333;
  line-height: 45rpx;
}

.foreshow-footer {
  display: flex;
  justify-content: space-between;
  padding: 10rpx 20rpx 28rpx 20rpx;
}

.foreshow-footer-cnt {
  display: flex;
  align-items: center;
}

.footer {
  background-color: #fff;
  padding: 30rpx 0rpx;
  border-radius: 10rpx;
  margin:20rpx 0rpx 0rpx 0rpx;
}

.footer-cnt {
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-bottom:10rpx;
}

.footer-title {
  font-size: 32rpx;
  font-weight: bold;
  color: #333;
  line-height: 45rpx;
  margin-left: 10rpx;
}

.footer-img {
  width: 60rpx;
  height: 30rpx;
}

.footer-address {
  font-size: 26rpx;
  font-weight: 400;
  color: #666;
  line-height: 40rpx;
  text-align: center;
}

.footer-phone {
  color: #e6b854;
}

.foreshow-footer-cnt text {
  font-size: 26rpx;
  font-weight: 400;
  color: #999;
  line-height: 37rpx;
  margin-left: 10rpx;
}

.foreshow-time {
  width: 30rpx;
  height: 30rpx;
}

.index-say-lunbo {
  margin-bottom: 35rpx;
}

.dotActive {
  width: 32rpx;
  background-color: #f5854f;
}

.index-join {
  margin: 30rpx 0rpx 30rpx 0rpx;
  width: 687rpx;
  height: 154rpx;
}

.share-plan {
  width: 687rpx;
  height: 154rpx;
}

.index-join-cnt {
  margin: 30rpx 0rpx 0rpx 0rpx;
  width: 690rpx;
  height: 654rpx;
  background-color: rgb(248, 239, 223);
  box-shadow: 0rpx 0rpx 10rpx 0rpx rgb(236, 196, 126);
  display: flex;
  flex-direction: column;
  align-items: center;
  overflow: hidden;
}

.index-banner {
  margin: 40rpx 0rpx 40rpx 0;
  width: 690rpx;
  height: 238rpx;
}

.swiper-img {
  width: 100%;
  height: 100%;
}

.index-join-cnt-img1 {
  width: 654rpx;
  height: 314rpx;
  margin: 40rpx 0rpx;
}

.index-join-cnt-img2 {
  width: 477rpx;
  height: 68rpx;
}

.index-join-cnt-img3 {
  width: 480rpx;
  height: 132rpx;
  margin-top: 14rpx;
}

.hidden {
  border: 0;
  display: inline-block;
}

.index-share {
  width: 692rpx;
  height: 40rpx;
  margin-bottom: 36rpx;
}

.index-shareH-img {
  width: 618rpx;
  height: 390rpx;
  border-radius: 8rpx;
}

.index-share-title {
  width: 690rpx;
  height: 92rpx;
  font-size: 32rpx;
  color: #333;
  font-weight: bold;
  line-height: 48rpx;
  overflow: hidden;
}

.index-share-cnt {
  border-radius: 8rpx;
  background-color: #fff;
  margin-bottom: 45rpx;
  padding: 17px 36rpx;
}

.index-cnt {
  width: 690rpx;
  margin: 0 auto;
}

.index-planDetail {
  margin-top: 20rpx;
  display: flex;
  height: 124rpx;
  background-color: #f4d993;
  justify-content: space-around;
}

.index-planDetail-cnt {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.index-planDetail-explain {
  margin-top: 20rpx;
  position: relative;
  padding: 30rpx;
  border-radius: 8rpx;
  font-size: 28rpx;
  color: #333;
  line-height: 48rpx;
  overflow: hidden;
}
.index-sanjiao {
  top: -19rpx;
  left: 285rpx;
  position: relative;
  width: 0;
  height: 0;
  border-width: 0 20rpx 20rpx;
  border-style: solid;
  border-color: transparent transparent #e0e0e0;
}

.index-small-sanjiao {
  top: 2rpx;
  left: -20rpx;
  position: absolute;
  width: 0;
  height: 0;
  border-width: 0 20rpx 20rpx;
  border-style: solid;
  border-color: transparent transparent #fff;
}

.index-project {
  margin-top: 27rpx;
}

.index-project-cnt {
  display: flex;
  align-items: center;
}

.index-project-icon {
  width: 35rpx;
  height: 35rpx;
}

.index-project-txt {
  margin-left: 10rpx;
  font-size: 28rpx;
  color: #5b421b;
  line-height: 28px;
  white-space: nowrap;
  overflow: hidden;
}

.index-progress {
  display: flex;
  height: 124rpx;
  justify-content: space-between;
}

.progress-txt {
  font-size: 24rpx;
  color: #333;
  line-height: 33rpx;
}

progress {
  border: 1px solid hidden;
  border-radius: 10rpx;
  overflow: hidden;
  color: #f2f2f2;
}

.index-join-btn {
  margin-top: 20rpx;
  padding-top: 20rpx;
  border-top: 1px dashed #c8c7cc;
  display: flex;
  justify-content: flex-end;
}

.btn {
  width: 280rpx;
  height: 94rpx;
  line-height: 94rpx;
  margin: 0;
  padding: 0;
  background-color: #f5854f;
  color: #fff;
  font-size: 30rpx;
}

.num {
  font-size: 36rpx;
  font-weight: bold;
  color: #5b421b;
}

.num-txt {
  font-size: 26rpx;
  font-weight: normal;
  color: #5b421b;
}

.progress-num {
  font-size: 46rpx;
  color: #333;
  line-height: 53rpx;
}

.progress-num-txt {
  font-size: 26rpx;
  color: #333;
}

.index-reason {
  width: 690rpx;
  height: 610rpx;
  position: relative;
  margin-bottom: 35rpx;
}

.six-reason {
  position: absolute;
  top: 30rpx;
  left: 10rpx;
  width: 690rpx;
  height: 610rpx;
}

.bg-img {
  width: 690rpx;
  height: 610rpx;
}

.reason {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-left: 20rpx;
  margin-top: 30rpx;
  float: left;
  width: 200rpx;
  height: 231rpx;
  text-align: center;
}

.reason-img {
  width: 94rpx;
  height: 94rpx;
}

.reason-title {
  margin-bottom: 10rpx;
  height: 42rpx;
  font-size: 30rpx;
  font-weight: 500;
  color: #93713b;
  line-height: 42rpx;
  white-space: nowrap;
}

.reason-txt {
  font-size: 24rpx;
  font-weight: 400;
  color: #666;
  line-height: 36rpx;
  white-space: nowrap;
}
.invitation-img{
  margin-top:40rpx;
  width:690rpx;
  height:160rpx;
}
.index-duihuakuang{
  position: absolute;
  top:0;
  right:0;
  width:620rpx;
}
.index-duihuakuang-img{
    position: absolute;
  top:0;
  right:0;
  width:100%;
  height:100%;
}


