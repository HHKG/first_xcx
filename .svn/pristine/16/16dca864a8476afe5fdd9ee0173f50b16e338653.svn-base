/* pages/login/index/registe/registe.wxss */
.reg-body{
 width:92%;
  margin: 0 auto;
}
.reg-h{
  position: relative;
  display: flex;
  justify-content: center;
  margin-top:83rpx;
}
.reg-h::after{
 border:1px solid #333;
}
.reg-line{
  width:288.5rpx;
  border-bottom: 3rpx solid #B3B3B3;
}
.reg-img{
  position: absolute;
  display: flex;
  justify-content: space-between;
  left: 20rpx;
  top: -26rpx;
  width:650rpx;
}
.reg-img1{
  width:50rpx;
  height:50rpx;
}
.reg-img-cnt{
  display:flex;
  flex-direction: column;
  align-items: center;
}
.reg-img-cnt-txt{
  margin-top:10rpx;
  font-size: 24rpx;
  color:#333;
}
/* 填写手机号的样式：引用登录页面的样式*/
.index-h{
margin:158rpx 0 15rpx 0;
 width:210rpx;
height:59rpx;
font-size:42rpx;
font-weight:500;
color:#333;
line-height:59rpx;
}
.index-input{
  margin-top:50rpx;
  padding-bottom: 19rpx;
border-bottom:1rpx solid #979797;
}
input{
width:100%;
height:45rpx;
font-size:32rpx;
font-weight:400;
color:#333333;
line-height:45rpx;
}
button{
  width: 690rpx;
  height:102rpx;
  line-height: 102rpx;
  margin-top:42rpx;
  border-radius: 12rpx;
}
.index-btn{
 margin-top:33rpx;
 display: inline-block;
}
.btn1{
  background-color:#f0753a;
  color:#fff;
}
.btn2{
  background-color:#fff;
  color:#f0753a;
  border:1rpx solid #f0753a;
}
.index-position{
  position: relative;
}
.get-code{
  position: absolute;
  width:182rpx;
  height:62rpx;
  line-height: 62rpx;
  border-radius: 33rpx;
  border:1rpx solid #3A8782;
  color:#3A8782;
  font-size:28rpx;
  background:#fff;
  right:0;
  bottom:20rpx;
  z-index: 11
}
button::after {
  border: none;
}
.btn1[disabled]{
  background-color:#fab696!important;
  color:#fff!important;
}
.get-code[disabled]{
  background-color:#EBEBEB!important;
  color:#999999!important;
  border:0rpx;
}