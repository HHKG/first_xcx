.holiday-h {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 100rpx;
  border-top: 4rpx solid #e0e0e0;
}

.title {
  width: 50%;
  text-align: center;
  height: 100rpx;
  font-size: 32rpx;
  line-height: 100rpx;
  color: #666;
  font-weight: 400;
}

.active {
  font-size: 36rpx;
  height: 100rpx;
  line-height: 100rpx;
  color: #f5854f;
  border-bottom: 5rpx solid #f5854f;
}

.line {
  border-right: 1rpx solid #eee;
  height: 40rpx;
}

.holiday-banner {
  width: 750rpx;
  height: 180rpx;
  display: inline-block;
}

.holiday-banner-img {
  width: 100%;
  height: 100%;
}
.area{
  margin-bottom: 30rpx;
}

.area-name {
  margin-top: 9rpx;
  width: 690rpx;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  margin: 0 auto;
}

.area-name-cnt {
  margin-top: 21rpx;
  width: 162rpx;
  height: 60rpx;
  border-radius: 29rpx;
  background-color: #eee;
  font-size: 28rpx;
  color: #333;
  line-height: 60rpx;
  text-align: center;
}

.area-name-cnt-active {
  background-color: #f5854f;
  color: #fff;
}

.area-cnt {
  width: 690rpx;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  margin: 0 auto;
}

.area-cnt-img {
  width: 100%;
}

.area-cnt-name {
  margin-top: 30rpx;
  position: relative;
  width: 335rpx;
  height:230rpx;
  border-radius: 10rpx;
  overflow: hidden;
}

.area-cnt-txt {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 1;
}

.area-cnt-txt1 {
  position: absolute;
  width: 138rpx;
  height: 40rpx;
  right: 30rpx;
  top: 30rpx;
  border-radius: 41rpx;
  background-color: #004b95;
  opacity: 0.2;
}

.area-cnt-txt2 {
  position: absolute;
  font-size: 36rpx;
  color: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: 100%;
}

.area-cnt-txt3 {
  position: absolute;
  right: 50rpx;
  top: 35rpx;
  font-size: 24rpx;
  color: #fff;
}
.holiday-plan{
display: flex;
align-items: center;
flex-direction: column;
width:690rpx;
height:1306rpx;
background:rgba(255,243,219,1);
box-shadow:2rpx 2rpx 10rpx 0rpx rgba(255,224,161,1);
border-radius:8rpx;
margin:40rpx auto;
padding:30rpx 0rpx;
}
.holiday-plan-img1{
  width:100rpx;
  height:100rpx;
}
.gong-ge{
  width: 690rpx;
  margin: 0 auto;
  display: flex;
  flex-wrap: wrap;
  justify-content:space-between;
}
.holiday-ensure{
  display: flex;
  flex-direction: column;
  padding:30rpx 20rpx;
}
.holiday-ensure-h{
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 34rpx;
  font-weight: 500;
  line-height: 40rpx;
  color:#333;
}
.holiday-ensure-h-txt{
  margin-left:14rpx;
}
.holiday-ensure-f{
  margin-top:10rpx;
  font-size: 26rpx;
  color:#666;
}
.holiday-ensure-h-img{
  width:74rpx;
  height:74rpx;
}
.holiday-ensure{
  margin-bottom:30rpx;
  width:290rpx;
height:220rpx;
background:rgba(255,255,255,1);
box-shadow:0rpx 4rpx 2rpx 0rpx rgba(0,0,0,0.3),0rpx -1rpx 1rpx 0rpx rgba(0,0,0,0.11);
border-radius:8rpx;
}
.holiday-swiper-item{
width:100%;
height:100%;
background:#fff;
box-shadow:0rpx 5rpx 2rpx 0rpx rgba(0,0,0,0.3),0rpx -1rpx 1rpx 0rpx rgba(0,0,0,0.11);
border-radius:12px;

}
.holiday-swiper{
  margin:30rpx auto 0;
   width:690rpx;
   height: 690rpx;
}
.holiday-dots{
  margin-top:30rpx;
  display: flex;
  justify-content: center;
}
.holiday-dot{
  width:18rpx;
  height:8rpx;
  background-color:#E0E0E0;
  margin-left: 10rpx;
}
.dotActive{
  width:40rpx;
  background-color:#f5854f;
}
.evaluate{
  margin-top:28rpx;
  width:100%;
  text-align: center;
}
.evaluate-cnt{
