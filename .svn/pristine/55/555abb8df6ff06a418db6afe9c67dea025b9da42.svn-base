font-weight:400;
color:#666666;
line-height:33rpx;
}
.evaluate-address{
  display: flex;
  align-items: center;
}
.evaluate-address-img{
  width:20rpx;
  height:28rpx;
}
.evaluate-address-txt{
  margin-left:10rpx;
height:40rpx;
font-size:28rpx;
font-weight:400;
color:#333;
line-height:40rpx;
}
.evaluate-words-img{
  width:29rpx;
  height:20rpx;
}
.evaluate-words{
  display: flex;
  flex-direction: column;
  width:630rpx;
  padding:20rpx;
  background-color:#F7F7F7;
}
.evaluate-words-txt{
  text-align: start;
font-size:26rpx;
font-weight:400;
color:rgba(102,102,102,1);
line-height:46rpx;
}
.evaluate-h{
