  align-items: center;
}
.evaluate-h-img{
  width:100rpx;
  height:100rpx;
  border-radius: 101rpx;
}
.evaluate-nickname{
height:37rpx;
font-size:26rpx;
font-weight:400;
color:#333;
line-height:37rpx;
}
.evaluate-txt{
height:33rpx;
font-size:24rpx;
