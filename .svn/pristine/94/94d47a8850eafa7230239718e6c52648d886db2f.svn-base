/* pages/login/index/index.wxss */
@import "/pages/member/index.wxss";
.index-body{
  width:690rpx;
 margin-left:30rpx;
  position: absolute;
  top:0;
  left: 0;

}
.index-h{
margin:58rpx 0 15rpx 0;
 width:210rpx;
height:59rpx;
font-size:42rpx;
font-weight:500;
color:#fff;
line-height:59rpx;
}
.index-input{
  margin-top:30rpx;
  padding-bottom: 19rpx;
border-bottom:1rpx solid #fff;
}
input{
width:100%;
height:45rpx;
font-size:32rpx;
font-weight:400;
color:#fff;
line-height:45rpx;
}
button{
  width: 690rpx;
  height:102rpx;
  margin-top:30rpx;
  line-height: 102rpx;
  border-radius: 12rpx;
}
.index-btn{
 display: inline-block;
}
.btn1{
  background-color:#f0753a;
  color:#fff;
}
.placeholder{
  color:#fff;
}
.btn2{
  color:#fff;
  background-color: transparent;
  border:1rpx solid #fff;
}
.index-position{
  position: relative;
}
.get-code{
  position: absolute;
  width:182rpx;
  height:62rpx;
  line-height: 62rpx;
  border-radius: 33rpx;
  border:1rpx solid #fff;
  color:#fff;
  font-size:28rpx;
  background-color: transparent;
  right:0;
  bottom:20rpx;
  z-index: 11
}
button::after {
  border: none;
}
.btn1[disabled]{
  background-color:#fab696!important;
  color:#fff!important;
}
.get-code[disabled]{
  background-color:#EBEBEB!important;
  color:#999999!important;
  border:0rpx;
}
.login-bg-img{
  width:750rpx;
}
.login-cnt{
  position: relative;
}
.login-logo-img{
  margin-top:94rpx;
  width:240rpx;
  height:71rpx;
}
.wechat{
  width:84rpx;
  height:84rpx;
}
.other-way{
  height: 84rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.left-line{
  border-top:1rpx solid #fff;
  width:141rpx;
}
.right-line{
  border-top:1rpx solid #fff;
  width:141rpx;
}
.other-way-txt{
height:40rpx;
font-size:28rpx;
color:rgba(255,255,255,1);
line-height:40rpx;
margin:0 20rpx;
}
.other{
  margin-top:184rpx;
  display: flex;
  flex-direction: column;
 align-items: center;
}