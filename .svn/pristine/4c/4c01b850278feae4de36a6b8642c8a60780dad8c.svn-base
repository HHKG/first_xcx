@import "/pages/member/index.wxss";
.img {
  width: 100%;
  height: 100%;
}

swiper {
  height: 100%;
}

.m-apply-btn {
  position: fixed;
  right: 0;
  top: 326rpx;
  z-index: 1;
  display: flex;
}
.contact{
  position: fixed;
  right: 0;
  top: 20rpx;
  z-index: 1;
  display: flex;
}

.m-apply-btn-img {
  width: 70rpx;
  height: 236rpx;
}

.apply-btn-img {
  width: 470rpx;
  height: 88rpx;
}

.m-progress {
  width: 100%;
}

.img-join {
  width: 100%;
  height: 100%;
  z-index: -1;
  position: relative;
}

.form-title {
  display: flex;
  align-items: center;
  width: 100%;
  height: 200rpx;
  color: #fff;
  font-size: 24rpx;
}

.form-title-layout {
  margin: 0 auto;
  width: 80%;
}

.form-progress {
  border-radius: 100rpx;
  overflow: hidden;
}

.form-percent {
  color: #f5ce87;
  font-size: 36rpx;
}

.form-progress-num {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 66rpx;
  height: 41rpx;
  background: rgba(27, 78, 81, 1);
  color: #fff;
  font-size: 28rpx;
}

.form-sanjiao {
  width: 0;
  height: 0;
  border-top: 10rpx solid rgba(27, 78, 81, 1);
  border-left: 10rpx solid transparent;
  border-right: 10rpx solid transparent;
}

.form-prompt {
  width: 66rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-left: 63%;
}

.img-layout {
  width: 100%;
  position: absolute;
  top: 0;
  left: 0;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-around;
}

