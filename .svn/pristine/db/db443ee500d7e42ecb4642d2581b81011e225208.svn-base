/* pages/project/cases/cases.wxss */
page{
  padding-bottom:122rpx;
}
.cases-planDetail {
  position:relative;
  margin: 0 auto;
  width:690rpx;
  display: flex;
  height: 142rpx;
  justify-content: space-around;
  background:rgba(255,255,255,1);
border-radius:4px;
 border:2rpx solid rgba(230,230,230,1);
}

.cases-planDetail-cnt {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.num {
  font-size: 36rpx;
  font-weight: bold;
  color: #5B421B;
}
.cases-select-icon{
  position: absolute;
  right:0rpx;
  bottom:0rpx;
  width:71rpx;
  height:62rpx;
  /* display: none; */
}

.num-txt {
  font-size: 26rpx;
  font-weight: normal;
  color: #5b421b;
}
.cases-name-color{
  color:#333!important;
}

.cases-case-name {
  margin:30rpx 0 15rpx 30rpx;
  font-size: 32rpx;
  color: #999;
}
.cases-chioce{
  margin-left:30rpx;
font-size:34rpx;
color:rgba(102,102,102,1);
}
.cases-title{
height: 130rpx;
font-size:46rpx;
font-weight:600;
color:rgba(51,51,51,1);
text-align: center;
line-height: 130rpx;
}
.cases-active{
  border:2rpx solid #F5854F;
}
.cases-nav{
  width:750rpx;
  height:92rpx;
  text-align: center;
  line-height: 92rpx;
  color:#fff;
  font-size: 30rpx;
  background-color:#F5854F;
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 5;
}
