<!--pages/member/cash/authentication/authentication.wxml-->
<view>
  <view style="height:76rpx;" >
    <import src="/pages/template/header/header" />
    <template is="header" data='{{pattern}}'></template>
  </view>
  <view class="aut-title">我们将会通过第三方认证您的身份信息</view>
  <view class="aut-h">
    <view class="aut-cnt">
      <text class="aut-cnt-txt">姓名</text>
      <input class="aut-cnt-input" type="text" value="" placeholder='请输入您的姓名' bindinput='nameinput'></input>
      <view class="aut-line"></view>
    </view>
    <view class="aut-cnt">
      <text class="aut-cnt-txt">身份证号</text>
      <input class="aut-cnt-input" type="idcard" value="" placeholder='请输入您的身份证号码' bindinput='cardinput'></input>
    </view>
  </view>
  <button bindtap='Land'>下一步</button>
</view>