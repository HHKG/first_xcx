@import "../../wxParse/wxParse.wxss";
@import "/pages/member/index.wxss";

page {
  background: #fafafa;
  padding-bottom: 98rpx;
}

.project-detail-cnt {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  position: relative;
  height: 110rpx;
}

.project-planDetail-explain {
  margin-top: 35rpx;
  position: relative;
  padding: 23rpx;
  border: 1rpx solid #e0e0e0;
  border-radius: 8rpx;
  font-size: 28rpx;
  color: #333;
  line-height: 48rpx;
}
navigator > image{
  border:1px solid red;
  width:365px;
  height:159.625px;
}
