/* pages/member/index.wxss */
page{
  background-color:#FAFAFA;
}
.member-h {
  display: flex;
  align-items: flex-end;
  justify-content: center;
  width: 750rpx;
  height: 330rpx;
  background-color: #092321;
}

.member-card-img-position {
  width: 690rpx;
  position: relative;
  height: 276rpx;
}

.member-card-img {
  width: 690rpx;
  height: 276rpx;
}

.member-jioabiao-img {
  position: absolute;
  width: 232rpx;
  height: 56rpx;
  top: 0;
  right: 0;
}

.member-touxiang {
  width: 109rpx;
  height: 109rpx;
  border-radius: 110rpx;
}

.member-card-cnt {
  position: absolute;
  top: 38rpx;
  left: 38rpx;
  width: 614rpx;
  height: 200rpx;
}

.member-card-txt {
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  height: 109rpx;
  margin-left: 26rpx;
}

.member-name {
  font-size: 32rpx;
  color: #4d4122;
  line-height: 45rpx;
  white-space: nowrap;
}

.member-phone {
  font-size: 28rpx;
  color: #4d4122;
  line-height: 40rpx;
}

.member-card-h {
  display: flex;
}

.member-card-val {
  display: flex;
  justify-content: space-between;
  height: 80rpx;
  margin-top:28rpx;
 align-items: flex-end;
}

.member-qian-val {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.qian-val-num {
  font-size: 40rpx;
  color: #333;
  font-weight: bold;
  line-height: 47rpx;
}

.qian-val-txt {
  font-size: 24rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(113, 91, 34, 1);
  line-height: 33rpx;
}
.card-num{
font-size:26rpx;
font-weight:bold;
color:#000;
line-height:37rpx;
}
.member-grade{
  display: flex;
  justify-content: space-between;
  align-items: center;
  height:98rpx;
  background-color:#496966;
  padding:0 30rpx;
}
.member-grade-img{
  width:186rpx;
  height:56rpx;
}
.member-grade-txt{
font-size:28rpx;
color:#EBD9A0;
line-height:40rpx;
margin-right:10rpx;
}
.member-right-img{
  width:12rpx;
  height:24rpx;
}
.member-recharge{
  display: flex;
  height:24rpx;
  align-items: center;
}
.member-gongge{
  height:210rpx;
  background-color:#fff;
  margin-top:32rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding:0 55rpx;
}
.member-plan-img{
  width:85rpx;
  height:85rpx;
}
.member-gongge-cnt{
  width:140rpx;
  display: flex;
  flex-direction:column;
  align-items: center;
}
.member-gongge-txt{
  margin-top:10rpx;
font-size:28rpx;
color:#333;
line-height:40rpx;
}
.member-banner{
  margin:30rpx;
  width:690rpx;
  height:194rpx;
}